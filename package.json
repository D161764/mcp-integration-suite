{
    "name": "mcp-integration-suite",
    "version": "1.0.0",
    "description": "MCP Server fÃ¼r Integration Package Management",
    "main": "dist/server.js",
    "scripts": {
        "build:generateclient": "node src/scripts/buildOdataClient.js",
        "build:tsc": "tsc -p tsconfig.json",
        "build": "npm run build:generateclient && npm run build:tsc",
        "start": "node dist/server.js",
        "start:stdio": "node dist/index.js",
        "dev": "npm run build:tsc && npm run dev:inspector",
        "dev:inspector": "npx @modelcontextprotocol/inspector node ./dist/index.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "watch:log": "tail -n 20 -F ~/Library/Logs/Claude/mcp-server-mcp-integration-suite.log",
        "postinstall": "npm run build"
    },
    "dependencies": {
        "@modelcontextprotocol/sdk": "^1.8.0",
        "@sap-cloud-sdk/connectivity": "^4.1.2",
        "@sap-cloud-sdk/http-client": "^4.1.2",
        "@sap-cloud-sdk/odata-v2": "^4.1.2",
        "@sap-cloud-sdk/temporal-de-serializers": "^4.1.2",
        "@sap-cloud-sdk/test-util": "^4.1.2",
        "adm-zip": "^0.5.16",
        "axios": "^1.8.4",
        "dotenv": "^17.2.3",
        "express": "^4.21.2",
        "glob": "^11.0.1",
        "puppeteer": "^24.5.0",
        "rimraf": "^6.0.1",
        "semver": "^7.7.1",
        "swagger-ui-express": "^5.0.1",
        "zod": "^4.1.12"
    },
    "devDependencies": {
        "@sap-cloud-sdk/generator": "^4.1.2",
        "@types/adm-zip": "^0.5.7",
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.10.0",
        "@types/puppeteer": "^5.4.7",
        "@types/semver": "^7.7.0",
        "@types/swagger-ui-express": "^4.1.8",
        "jest": "^29.7.0",
        "ts-jest": "^29.3.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.0.0"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=9.0.0"
    }
}
